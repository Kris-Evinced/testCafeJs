{
    "uploadToPlatformOptions": {
        "enableUploadToPlatform": true,
        "setUploadToPlatformDefault": false
    },
    "enableScreenshots": true,
    "skipValidations": [
        {
            "selector": "*",
            "urlRegex": ".*demo.evinced.com\/",
            "validationTypes": [
                "COMPOSITE_NOT_IN_FOCUS_SEQUENCE"
            ]
        }
    ],
    "logging": {
        "LOGGING_LEVEL": "info",
        "ADD_LOGGING_CONTEXT": true
    },
    "sdkLogging": {
        "enable": true, 
        "level": "debug", 
        "directory": "./logs", 
        "browserLoggingEnabled": true, 
        "systemInformationLoggingEnabled": true, 
        "maxLogLength": 1000 
    }
}